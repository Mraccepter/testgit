package 未实现算法;
//查找子字符串
public class KMP {
//暴力查找子字符串时间复杂度为~NM
	public static int search(String pat, String txt){
		int M = pat.length();
		int N = txt.length();
		//这一行不是应该小于N-M？
		for(int i = 0; i <= N-M; i++){
			int j ;
			for(j = 0; j < M; i++){
				if(txt.charAt(i+j) != pat.charAt(j))
					break;
			}
			if(j == M) return i;
		}
		return N;
	}

}
