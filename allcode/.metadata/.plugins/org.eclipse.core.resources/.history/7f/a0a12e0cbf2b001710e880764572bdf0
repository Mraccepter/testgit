import java.util.Arrays;
import java.util.Scanner;

public class Main {
	public static void main(String[] args) {
		
		Scanner sc = new Scanner(System.in);
		
		int N = sc.nextInt();
		int[] nums = new int[N];
		for(int i = 0; i < N; i++) {
			nums[i] = sc.nextInt();
		}
		Arrays.sort(nums);
		
		int num = nums[0];
		
		int n = 1;
		for(int i = 1; i < N; i++) {
			if(nums[i] != num) {
				num = nums[i];
				n++;
			}
			if(n == 3) {
				break;
			}
		}
		if(n == 3){
			System.out.println(num);
		}
		else{
			System.out.println(-1);
		}
			
	}
}